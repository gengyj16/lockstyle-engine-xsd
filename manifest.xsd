<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="qualified">
    <xs:element name="Lockscreen">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Wallpaper" />
                <xs:element ref="ExternalCommands" />
                <xs:element ref="Weather" />
                <xs:element ref="Calendar" />
                <xs:element ref="Var" />
                <xs:element ref="VarArray" />
                <xs:element ref="Group" />
                <xs:element ref="Frame" />
                <xs:element ref="Image" />
                <xs:element ref="SourceImage" />
                <xs:element ref="ImageNumber" />
                <xs:element ref="ImageSeries" />
                <xs:element ref="WaterWallpaper" />
                <xs:element ref="Time" />
                <xs:element ref="DateTime" />
                <xs:element ref="Slider" />
                <xs:element ref="Button" />
                <xs:element ref="SupportPictorialButton" />
                <xs:element ref="SliderEx" />
                <xs:element ref="Unlocker" />
                <xs:element ref="VariableBinders" />
                <xs:element ref="UnlockerView" />
                <xs:element ref="MusicControl" />
                <xs:element ref="Video" />
                <xs:element ref="VirtualScreen" />
                <xs:element ref="UserDefineText" />
                <xs:element ref="ParticleView" />
                <xs:element ref="Graph" />
                <xs:element ref="MeshImage" />
                <xs:element ref="Rectangle" />
                <xs:element ref="Circle" />
                <xs:element ref="Ellipse" />
                <xs:element ref="Line" />
                <xs:element ref="Arc" />
                <xs:element ref="Marquee" />
                <xs:element ref="FlipBook" />
                <xs:element ref="CountDownTime" />
                <xs:element ref="VortexView" />
                <xs:element ref="DropView" />
                <xs:element ref="Array" />
                <xs:element ref="VariableFramerate" />
                <xs:element ref="Situations" />
                <xs:element ref="Fold" />
                <xs:element ref="ParticleScatter" />
                <xs:element ref="PathUtil" />
                <xs:element ref="PathRun" />
                <xs:element ref="ImageFilterView" />
                <xs:element ref="ImageParticleView" />
                <xs:element ref="Bloom" />
                <xs:element ref="DropPhysicalView" />
                <xs:element ref="Text" />
                <xs:element ref="Geometry" />
                <xs:element ref="VR" />
                <xs:element ref="WindowParallax" />
                <xs:element ref="ScaleParallax" />
                <xs:element ref="MultiLayer" />
                <xs:element ref="ARBase" />
                <xs:element ref="GameFrame" />
                <xs:element ref="StereoView" />
                <xs:element ref="FluidsView" />
                <xs:element ref="StereoDropView" />
                <xs:element ref="MediaControl" />
                <xs:element ref="LrcView" />
                <xs:element ref="MediaIcon" />
                <xs:element ref="MediaProgressbar" />
                <xs:element ref="Healthy" />
                <xs:element ref="Pets" />
                <xs:element ref="PetGroup" />
                <xs:element ref="PetSkill" />
                <xs:element ref="CollisionWord" />
                <xs:element ref="PressingAPNGView" />
                <xs:element ref="FluidView3D" />
                <xs:element maxOccurs="1" minOccurs="0" ref="ScheduleElement" />
                <xs:element ref="EditGroup" />
                <xs:element ref="EditText" />
                <xs:element maxOccurs="1" minOccurs="0" ref="BluetoothBattery" />
                <!-- <xs:any processContents="lax" /> -->
            </xs:choice>
            <xs:attribute name="frameRate" default="60" />
            <xs:attribute name="screenWidth" type="numType" />
            <xs:attribute name="screenHeight" type="numType" />
            <xs:attribute name="vibrate" type="booleanType" />
            <xs:attribute name="version" type="numType" />
            <xs:attribute name="xsdVersion" type="numType" />
            <xs:attribute name="pressure" type="booleanType" />
            <xs:attribute name="displayDesktop" type="booleanType" />
            <xs:attribute name="chargeAnimTime" default="9" type="xs:integer" />
            <xs:attribute name="holderTextColor" type="colorType" />
        </xs:complexType>
    </xs:element>
    <xs:element name="CollisionWorld" substitutionGroup="CollisionWord" />
    <xs:element name="Texture" substitutionGroup="Image" />
    <xs:element name="Root" substitutionGroup="Lockscreen" />
    <xs:element name="Diy" substitutionGroup="Lockscreen" />
    <xs:element name="Widget" substitutionGroup="Lockscreen" />
    <xs:element name="CommonWallpaper" substitutionGroup="Lockscreen" />
    <xs:element name="BinderList" substitutionGroup="VariableBinders" />

    <xs:element name="PressingAPNGView" type="imageType" />

    <!-- 编辑组元素组件，可以把编辑框元素包含在组中，同时控制显示隐藏，设置动画等 -->
    <xs:element name="EditGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="EditText" />
            </xs:choice>
            <xs:attribute name="name" type="stringType" />
            <xs:attribute name="x" default="0" type="expression" />
            <xs:attribute name="y" default="0" type="expression" />
            <xs:attribute name="w" default="0" type="expression" />
            <xs:attribute name="h" default="0" type="expression" />
            <xs:attribute name="width" default="0" type="expression" />
            <xs:attribute name="height" default="0" type="expression" />
            <xs:attribute name="rotation" default="0" type="expression" />
            <xs:attribute name="rotationX" default="0" type="expression" />
            <xs:attribute name="rotationY" default="0" type="expression" />
            <xs:attribute name="angle" default="0" type="expression" />
            <xs:attribute name="angleX" default="0" type="expression" />
            <xs:attribute name="angleY" default="0" type="expression" />
            <xs:attribute name="alpha" default="255" type="expression" />
            <xs:attribute name="active" default="1" type="expression" />
            <xs:attribute name="visibility" default="1" type="expression" />
            <xs:attribute name="layered" type="expression" />
            <xs:attribute name="clip" type="booleanType" />
            <xs:attribute name="align" type="stringType" />
            <xs:attribute name="alignV" type="stringType" />
            <xs:attribute name="hint" type="stringType" use="required" />
        </xs:complexType>
    </xs:element>

    <!-- 编辑框 -->
    <xs:element name="EditText">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:group ref="animationGroup" />
                <xs:element ref="PathItem" />
            </xs:choice>
            <xs:attributeGroup ref="viewCom" />
            <xs:attribute name="size" type="expression" />
            <xs:attribute name="color" type="colorType" />
            <xs:attribute name="bold" type="booleanType" />
            <xs:attribute name="shadowColor" type="colorType" />
            <xs:attribute name="shadowDx" type="numType" />
            <xs:attribute name="shadowDy" type="numType" />
            <xs:attribute name="Radius" type="numType" />
            <xs:attribute name="marquee" type="booleanType" />
            <xs:attribute name="showTime" type="expression" />
            <xs:attribute name="showStyle" default="0" type="expression" />
            <xs:attribute name="tileMode" type="stringType" />
            <xs:attribute name="marqueeColor" type="colorType" />
            <xs:attribute name="spaceTimes" default="1" type="numType" />
            <xs:attribute name="spaceExtraAdd" default="0" type="numType" />
            <xs:attribute name="canPath" type="booleanType" />
            <xs:attribute name="run" type="expression" />
            <xs:attribute name="speed" type="expression" />
            <xs:attribute name="autoLineFeed" type="booleanType" />
            <xs:attribute name="format" type="stringType" />
            <xs:attribute name="paras" type="stringType" />
            <xs:attribute name="text" type="xs:string" />
            <xs:attribute name="textExp" type="stringType" />
            <xs:attribute name="textalign" type="stringType" />
            <xs:attribute name="scrollDisplay" type="booleanType" />
            <xs:attribute name="marqueeRepeatLimit" type="numType" />
            <xs:attribute name="clickable" type="booleanType" />
            <xs:attribute name="delayTime" default="3000" type="numType" />
            <xs:attribute name="editId" type="numType" use="required" />
            <xs:attribute name="hint" type="stringType" />
        </xs:complexType>
    </xs:element>

    <!--日程声明 -->
    <xs:element name="ScheduleElement" />

    <!--蓝牙耳机电量 -->
    <xs:element name="BluetoothBattery" />

    <xs:element name="StereoDropView">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="StereoImage" />
                <xs:element ref="StereoDropItem" />
                <xs:element ref="SourcesAnimation" />
            </xs:choice>
            <xs:attribute name="gravityX" type="expression" />
            <xs:attribute name="gravityY" type="expression" />
            <xs:attribute name="gravityZ" type="expression" />
            <xs:attribute name="visibility" type="expression" />
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
            <xs:attribute name="width" type="expression" />
            <xs:attribute name="height" type="expression" />
            <xs:attribute name="depth" type="xs:float" />
            <xs:attribute name="bgSrc" type="stringType" />
        </xs:complexType>
    </xs:element>

    <xs:element name="StereoImage">
        <xs:complexType>
            <xs:attribute name="x" type="xs:float" />
            <xs:attribute name="y" type="xs:float" />
            <xs:attribute name="depth" type="xs:float" />
            <xs:attribute name="src" type="stringType" />
            <xs:attribute name="visibility" type="expression" />
        </xs:complexType>
    </xs:element>

    <xs:element name="StereoDropItem">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="ItemVelocity" />
                <xs:element ref="ItemPosition" />
                <xs:element ref="SourcesAnimation" />
                <xs:element ref="ItemConvergence" />
            </xs:choice>
            <xs:attribute name="maxCount" type="xs:int" />
            <xs:attribute name="src" type="stringType" />
            <xs:attribute name="buildSpeed" type="xs:float" />
            <xs:attribute name="visibility" type="expression" />
            <xs:attribute name="size" type="xs:int" />
        </xs:complexType>
    </xs:element>

    <xs:element name="ItemConvergence">
        <xs:complexType>
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
            <xs:attribute name="z" type="expression" />
            <xs:attribute name="velocity" type="stringType" />
        </xs:complexType>
    </xs:element>

    <xs:element name="ItemVelocity">
        <xs:complexType>
            <xs:attribute name="isRandom" type="booleanType" />
            <xs:attribute name="velocityX" type="xs:float" />
            <xs:attribute name="velocityY" type="xs:float" />
            <xs:attribute name="velocityZ" type="xs:float" />
            <xs:attribute name="lowestVelocityX" type="xs:float" />
            <xs:attribute name="lowestVelocityY" type="xs:float" />
            <xs:attribute name="lowestVelocityZ" type="xs:float" />
        </xs:complexType>
    </xs:element>

    <xs:element name="ItemPosition">
        <xs:complexType>
            <xs:attribute name="isRandom" type="booleanType" />
            <xs:attribute name="x" type="xs:float" />
            <xs:attribute name="y" type="xs:float" />
            <xs:attribute name="z" type="xs:float" />
        </xs:complexType>
    </xs:element>
    <!-- 运动健康 -->
    <xs:element name="Healthy">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Var" />
            </xs:choice>
            <xs:attribute name="polling" type="booleanType" />
            <xs:attribute name="time" type="xs:double" />
        </xs:complexType>
    </xs:element>
    <!-- 流体动效 -->
    <xs:element name="FluidsView">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="CircleShape" />
                <xs:element ref="PolygonShape" />
            </xs:choice>
            <xs:attribute name="color" type="colorType" />
            <xs:attribute name="gravityRatio" type="expression" />
            <xs:attribute name="viscosity" type="expression" />
            <xs:attribute name="waterAlpha" type="expression" />
            <xs:attribute name="waterAlphaThreshold" type="expression" />
            <xs:attribute name="bgSrc" type="stringType" />
            <xs:attribute name="scaleType" type="stringType" />
            <xs:attribute name="srcid" type="expression" />
            <xs:attribute name="persist" type="booleanType" />
        </xs:complexType>
    </xs:element>

    <!-- 圆形shape -->
    <xs:element name="CircleShape">
        <xs:complexType>
            <xs:attributeGroup ref="shapeCom" />
            <xs:attribute name="radius" type="expression" />
        </xs:complexType>
    </xs:element>

    <!-- 方形的shape -->
    <xs:element name="PolygonShape">
        <xs:complexType>
            <xs:attributeGroup ref="shapeCom" />
            <xs:attribute name="hx" type="expression" />
            <xs:attribute name="hy" type="expression" />
            <xs:attribute name="angle" type="expression" />
        </xs:complexType>
    </xs:element>

    <!-- shape的公用属性 -->
    <xs:attributeGroup name="shapeCom">
        <xs:attribute name="type" type="stringType" />
        <xs:attribute name="xPosition" type="expression" />
        <xs:attribute name="yPosition" type="expression" />
    </xs:attributeGroup>

    <xs:element name="StereoView">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="StereoGroup" />
            </xs:choice>
            <xs:attributeGroup ref="viewCom" />
            <xs:attribute name="sAngle" type="xs:double" />
            <xs:attribute name="can3D" type="xs:double" />
            <xs:attribute name="resistance" type="xs:double" />
            <xs:attribute name="fixed" type="xs:int" />
        </xs:complexType>
    </xs:element>

    <xs:element name="ScrawlView">
        <xs:complexType>
            <xs:attributeGroup ref="viewCom" />
            <xs:attribute name="paintColor" type="expression" />
            <xs:attribute name="paintWidth" type="expression" />
            <xs:attribute name="paintType" type="expression" />
            <xs:attribute name="back" type="expression" />
            <xs:attribute name="save" type="expression" />
        </xs:complexType>
    </xs:element>

    <xs:element name="StereoGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Image" />
                <xs:element ref="Text" />
                <xs:element ref="Button" />
                <xs:element ref="EditGroup" />
                <xs:element ref="EditText" />
            </xs:choice>
        </xs:complexType>
    </xs:element>

    <xs:element name="Slice">
        <xs:complexType>
            <xs:attribute name="time" type="stringType" />
            <xs:attribute name="duration" type="stringType" />
            <xs:attribute name="type" type="stringType" />
            <xs:attribute name="intensity" type="stringType" />
            <xs:attribute name="sharpness" type="stringType" />
        </xs:complexType>
    </xs:element>
    <xs:element name="Sharpness">
        <xs:complexType>
            <xs:attribute name="time" type="stringType" />
            <xs:attribute name="parameters" type="stringType" />
        </xs:complexType>
    </xs:element>
    <xs:element name="Intensitiy">
        <xs:complexType>
            <xs:attribute name="time" type="stringType" />
            <xs:attribute name="parameters" type="stringType" />
        </xs:complexType>
    </xs:element>
    <xs:element name="Wave">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Slice" />
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="Adjustcurve">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Sharpness" />
                <xs:element ref="Intensitiy" />
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="Vibrate">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Wave" />
                <xs:element ref="Adjustcurve" />
            </xs:choice>
            <xs:attribute name="loop" type="stringType" />
        </xs:complexType>
    </xs:element>
    <xs:element name="VibrateCommand">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Vibrate" />
            </xs:choice>
            <xs:attribute name="id" type="stringType" />
            <xs:attribute name="type" type="stringType" />
            <xs:attribute name="define" type="stringType" />
            <xs:attribute name="adid" type="stringType" />
        </xs:complexType>
    </xs:element>
    <xs:element name="RefreshWeatherCommand"></xs:element>

    <xs:element name="CollBody">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="IntentCommand" />
                <xs:element ref="Texture" />
            </xs:choice>
            <xs:attribute name="id" type="stringType" />
            <xs:attribute name="type" type="expression" />
            <xs:attribute name="shape" type="expression" />
            <xs:attribute name="imgsrc" type="expression" />
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
            <xs:attribute name="width" type="expression" />
            <xs:attribute name="height" type="expression" />
            <xs:attribute name="radius" type="expression" />
            <xs:attribute name="friction" type="expression" />
            <xs:attribute name="restitution" type="expression" />
            <xs:attribute name="density" type="expression" />
            <xs:attribute name="forceX" type="expression" />
            <xs:attribute name="forceY" type="expression" />
            <xs:attribute name="vx" type="expression" />
            <xs:attribute name="vy" type="expression" />
            <xs:attribute name="isActive" type="expression" />
            <xs:attribute name="musicSrc" type="stringType" />
            <xs:attribute name="musickeepCur" type="stringType" />
            <xs:attribute name="musicloop" type="expression" />
            <xs:attribute name="vibrateTime" type="stringType" />
            <xs:attribute name="statevar" type="stringType" />
            <xs:attribute name="clickable" type="expression" />
            <xs:attribute name="gesture" type="expression" />
            <xs:attribute name="opx" type="stringType" />
            <xs:attribute name="opy" type="stringType" />
        </xs:complexType>
    </xs:element>

    <xs:element name="FluidView3D">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Model" />
                <xs:element ref="Fluid" />
            </xs:choice>
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
            <xs:attribute name="w" type="expression" />
            <xs:attribute name="h" type="expression" />
            <xs:attribute name="bgSrc" type="stringType" />
        </xs:complexType>
    </xs:element>

    <xs:element name="Model">
        <xs:complexType>
            <xs:attribute name="mx" type="stringType" />
            <xs:attribute name="my" type="stringType" />
            <xs:attribute name="mz" type="stringType" />
            <xs:attribute name="mscalex" type="stringType" />
            <xs:attribute name="mscaley" type="stringType" />
            <xs:attribute name="mscalez" type="stringType" />
            <xs:attribute name="mSrc" type="stringType" />
            <xs:attribute name="radianceSrc" type="stringType" />
            <xs:attribute name="irradianceSrc" type="stringType" />
        </xs:complexType>
    </xs:element>

    <xs:element name="Fluid">
        <xs:complexType>
            <xs:attribute name="fx" type="stringType" />
            <xs:attribute name="fy" type="stringType" />
            <xs:attribute name="fz" type="stringType" />
            <xs:attribute name="fscalex" type="stringType" />
            <xs:attribute name="fscaley" type="stringType" />
            <xs:attribute name="fscalez" type="stringType" />
            <xs:attribute name="fl" type="stringType" />
            <xs:attribute name="fw" type="stringType" />
            <xs:attribute name="fh" type="stringType" />
            <xs:attribute name="fradius" type="stringType" />
            <xs:attribute name="fshape" type="stringType" />
            <xs:attribute name="fedge" type="stringType" />
            <xs:attribute name="fvf" type="stringType" />
            <xs:attribute name="fgravity" type="stringType" />
            <xs:attribute name="fboundarySrc" type="stringType" />
            <xs:attribute name="fSrc" type="stringType" />
        </xs:complexType>
    </xs:element>

    <xs:element name="Fluid3dCommand">
        <xs:complexType>
            <xs:attribute name="fAddOrReduce" type="stringType" />
            <xs:attribute name="fSrc" type="stringType" />
            <xs:attribute name="bgSrc" type="stringType" />
            <xs:attribute name="fvf" type="stringType" />
        </xs:complexType>
    </xs:element>

    <xs:element name="CollisionWord">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="CollBody" />
                <xs:element ref="Texture" />
            </xs:choice>
            <xs:attribute name="vecX" type="expression" />
            <xs:attribute name="vecY" type="expression" />
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
            <xs:attribute name="width" type="expression" />
            <xs:attribute name="height" type="expression" />
            <xs:attribute name="defBourderLine" type="expression" />
            <xs:attribute name="accCoef" type="expression" />
            <xs:attribute name="minShake" type="expression" />
            <xs:attribute name="shakeCoef" type="expression" />
            <xs:attribute name="minMicroDb" type="expression" />
            <xs:attribute name="microCoef" type="expression" />
            <xs:attribute name="woldImgSrc" type="stringType" />
            <xs:attribute name="worldImgSrc" type="stringType" />
            <xs:attribute name="woldImgType" type="stringType" />
            <xs:attribute name="worldImgType" type="stringType" />
            <xs:attribute name="gwidth" type="expression" />
            <xs:attribute name="gheight" type="expression" />
            <xs:attribute name="visibility" type="expression" />
        </xs:complexType>
    </xs:element>

    <xs:element name="CollBodyCommand">
        <xs:complexType>
            <xs:attribute name="collbodyid" type="stringType" />
            <xs:attribute name="forceX" type="expression" />
            <xs:attribute name="forceY" type="expression" />
            <xs:attribute name="vx" type="expression" />
            <xs:attribute name="vy" type="expression" />
            <xs:attribute name="isActive" type="stringType" />
            <xs:attribute name="type" type="stringType" />
        </xs:complexType>
    </xs:element>
    <xs:element name="RefreshHealthyCommand">
        <xs:complexType></xs:complexType>
    </xs:element>
    <xs:element name="ProgressCommand">
        <xs:complexType>
            <xs:attribute name="mode" type="xs:int" />
            <xs:attribute name="change" type="xs:int" />
            <xs:attribute name="amplitude" type="xs:int" />
        </xs:complexType>
    </xs:element>
    <xs:element name="Calendar">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Var" />
            </xs:choice>
            <xs:attribute name="card" type="stringType" />
        </xs:complexType>
    </xs:element>

    <xs:element name="Weather">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Var" />
            </xs:choice>
            <xs:attribute name="globalPersist" type="stringType" />
        </xs:complexType>
    </xs:element>

    <xs:element name="GameFrame">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="PPsIds" />
            </xs:choice>
            <xs:attribute name="name" type="stringType" />
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
            <xs:attribute name="w" type="expression" />
            <xs:attribute name="h" type="expression" />
            <xs:attribute name="path" type="stringType" />
            <xs:attribute name="brightScreen" type="expression" />
            <xs:attribute name="visibility" type="expression" />
        </xs:complexType>
    </xs:element>

    <xs:element name="PPsIds">
        <xs:complexType>
            <xs:attribute name="adLocId" type="stringType" />
            <xs:attribute name="adLocName" type="stringType" />
            <xs:attribute name="size" type="stringType" />
            <xs:attribute name="type" type="stringType" />
            <xs:attribute name="visibility" type="expression" />
        </xs:complexType>
    </xs:element>

    <xs:element name="ARBase">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Modules" />
            </xs:choice>
            <xs:attribute name="bridgeAnim" type="stringType" />
            <xs:attribute name="skybox" type="stringType" />
            <xs:attribute name="diffuse" type="stringType" />
            <xs:attribute name="specular" type="stringType" />
            <xs:attribute name="modulePositionType" type="stringType" />
            <xs:attribute name="visibility" type="expression" />
            <xs:attribute name="setVideoPlayMode" type="stringType" />
            <xs:attribute name="chooseId" type="expression" />
            <xs:attribute name="openAnimation" type="expression" />
            <xs:attribute name="videoTime" type="xs:long" />
            <xs:attribute name="brightScreen" type="expression" />
            <xs:attribute name="mediaType" type="expression" />
        </xs:complexType>
    </xs:element>

    <xs:element name="Modules">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="BoneAnimation" />
                <xs:element ref="TranslateAnimation" />
            </xs:choice>
            <xs:attribute name="src" type="stringType" />
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
            <xs:attribute name="z" type="expression" />
            <xs:attribute name="id" type="xs:long" />
            <xs:attribute name="rotateX" type="xs:double" />
            <xs:attribute name="rotateY" type="xs:double" />
            <xs:attribute name="rotateZ" type="xs:double" />
            <xs:attribute name="scale" type="xs:double" />
            <xs:attribute name="name" type="stringType" />
            <xs:attribute name="fix" type="stringType" />
            <xs:attribute name="isRandom" type="stringType" />
            <xs:attribute name="scrollX" type="stringType" />
            <xs:attribute name="scrollY" type="stringType" />
        </xs:complexType>
    </xs:element>

    <xs:element name="BoneAnimation">
        <xs:complexType>
            <xs:attribute name="part" type="stringType" />
            <xs:attribute name="animation" type="stringType" />
        </xs:complexType>
    </xs:element>

    <xs:element name="TranslateAnimation">
        <xs:complexType>
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
            <xs:attribute name="z" type="expression" />
            <xs:attribute name="time" type="xs:double" />
        </xs:complexType>
    </xs:element>

    <xs:element name="MultiLayer">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Layer" />
            </xs:choice>
            <xs:attribute name="gravityX" type="expression" />
            <xs:attribute name="gravityY" type="expression" />
            <xs:attribute name="touchType" type="expression" />
            <xs:attribute name="stepX" type="expression" />
            <xs:attribute name="stepZ" type="expression" />
            <xs:attribute name="blur" type="expression" />
            <xs:attribute name="visibility" type="expression" />
            <xs:attribute name="pitchAngle" type="stringType" />
            <xs:attribute name="sidesAngle" type="stringType" />
        </xs:complexType>
    </xs:element>
    <xs:element name="Layer">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Source" />
            </xs:choice>
            <xs:attribute name="width" type="expression" />
            <xs:attribute name="w" type="expression" />
            <xs:attribute name="height" type="expression" />
            <xs:attribute name="h" type="expression" />
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
            <xs:attribute name="z" type="expression" />
            <xs:attribute name="src" type="stringType" />
            <xs:attribute name="staticCondition" type="stringType" />
            <xs:attribute name="delay" type="stringType" />
            <xs:attribute name="repeat" type="stringType" />
        </xs:complexType>
    </xs:element>
    <xs:element name="Source">
        <xs:complexType>
            <xs:attribute name="frame" type="stringType" />
            <xs:attribute name="time" type="xs:long" />
        </xs:complexType>
    </xs:element>
    <!-- WindowParallax -->
    <xs:element name="WindowParallax">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="LayerInfo" />
            </xs:choice>
            <xs:attribute name="posZ" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- ScaleParallax -->
    <xs:element name="ScaleParallax">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="LayerInfo" />
            </xs:choice>
            <xs:attribute name="translation" type="stringType" />
            <xs:attribute name="scale" type="stringType" />
            <xs:attribute name="radius" type="expression" />
            <xs:attribute name="rotateRate" type="expression" />
            <xs:attribute name="blur" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- LayerInfo -->
    <xs:element name="LayerInfo">
        <xs:complexType>
            <xs:attribute name="src" type="stringType" />
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
            <xs:attribute name="w" type="expression" />
            <xs:attribute name="h" type="expression" />
        </xs:complexType>
    </xs:element>
    <!--几何图形 -->
    <xs:element name="Geometry">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Rectangle" />
                <xs:element ref="Circle" />
                <xs:element ref="Ellipse" />
                <xs:element ref="Line" />
                <xs:element ref="Arc" />
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <!--外部自定义UnlockView组件解析:用于完成一些定制化的解锁方式和效果 -->
    <xs:element name="UnlockerView">
        <xs:complexType>
            <xs:attribute name="class" type="stringType" />
            <xs:attribute name="params" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!--音乐组件标签解析 -->
    <xs:element name="MusicControl">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element name="MusicImage" type="imageType" />
                <xs:element name="MusicText" type="textType" />
                <xs:element name="MusicButton" type="buttonType" />
            </xs:choice>
            <xs:attribute name="name" type="stringType" />
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
            <xs:attribute name="visibility" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 文本 -->
    <xs:element name="Text" type="textType" />
    <xs:element name="PathItem">
        <xs:complexType>
            <xs:attribute name="type" type="stringType" />
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
            <xs:attribute name="controlX" type="expression" />
            <xs:attribute name="controlY" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 图片 -->
    <xs:element name="Image" type="imageType" />
    <!-- 数字图片 -->
    <xs:element name="ImageNumber">
        <xs:complexType>
            <xs:group maxOccurs="unbounded" minOccurs="0" ref="animationGroup" />
            <xs:attributeGroup ref="viewCom" />
            <xs:attribute name="src" type="stringType" />
            <xs:attribute name="number" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 用于帧动画的组件 -->
    <xs:element name="SourceImage">
        <xs:complexType>
            <xs:group maxOccurs="unbounded" minOccurs="0" ref="animationGroup" />
            <xs:attributeGroup ref="viewCom" />
            <xs:attribute name="sourceName" type="stringType" />
            <xs:attribute name="format" type="stringType" />
            <xs:attribute name="from" type="expression" />
            <xs:attribute name="to" type="expression" />
            <xs:attribute name="loop" type="booleanType" />
            <xs:attribute name="space" type="xs:long" />
            <xs:attribute name="direction" type="numType" />
            <xs:attribute name="unlockTo" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 视频元素 -->
    <xs:element name="Video">
        <xs:complexType>
            <xs:attribute name="name" type="stringType" />
            <xs:attribute name="x" default="0" type="expression" />
            <xs:attribute name="y" default="0" type="expression" />
            <xs:attribute name="w" default="0" type="expression" />
            <xs:attribute name="h" default="0" type="expression" />
            <xs:attribute name="width" default="0" type="expression" />
            <xs:attribute name="height" default="0" type="expression" />
            <xs:attribute name="play" default="true" type="booleanType" />
            <xs:attribute name="looping" default="false" type="booleanType" />
            <xs:attribute name="count" type="expression" />
            <xs:attribute name="sound" default="0" type="numType" />
            <xs:attribute name="living" default="false" type="booleanType" />
            <xs:attribute name="src" type="stringType" />
            <xs:attribute name="scaleType" default="fill" />
            <xs:attribute name="visibility" type="expression" />
            <xs:attribute name="defaultBitmap" type="stringType" />
            <xs:attribute name="defaultBigBitmap" type="stringType" />
            <xs:attribute name="defaultBitmapMusic" type="stringType" />
            <xs:attribute name="isMusic" type="stringType" />
            <xs:attribute name="isBackground" type="stringType" />
            <xs:attribute name="turn" type="stringType" />
            <xs:attribute name="fold_expand_start" type="numType" />
            <xs:attribute name="fold_expand_end" type="numType" />
            <xs:attribute name="fold_collapse_big_start" type="numType" />
            <xs:attribute name="fold_collapse_big_end" type="numType" />
        </xs:complexType>
    </xs:element>
    <!-- 时间元素，自动使用时间数值图片和时间分隔符图片，组成时间元素 -->
    <xs:element name="Time">
        <xs:complexType>
            <xs:group maxOccurs="unbounded" minOccurs="0" ref="animationGroup" />
            <xs:attributeGroup ref="viewCom" />
            <xs:attribute name="src" type="stringType" />
            <xs:attribute name="space" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 日期元素 -->
    <xs:element name="DateTime">
        <xs:complexType>
            <xs:attributeGroup ref="viewCom" />
            <xs:attribute name="color" type="colorType" />
            <xs:attribute name="size" type="expression" />
            <xs:attribute name="format" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 用户签名组件，读取手机设置中的用户签名，达到用户自定义文字的效果 -->
    <xs:element name="UserDefineText">
        <xs:complexType>
            <xs:attribute name="name" type="stringType" />
            <xs:attribute name="x" default="0" type="expression" />
            <xs:attribute name="y" default="0" type="expression" />
            <xs:attribute name="w" default="0" type="expression" />
            <xs:attribute name="h" default="0" type="expression" />
            <xs:attribute name="width" default="0" type="expression" />
            <xs:attribute name="height" default="0" type="expression" />
            <xs:attribute name="alpha" default="255" type="expression" />
            <xs:attribute name="visibility" default="1" type="expression" />
            <xs:attribute name="color" type="colorType" />
            <xs:attribute name="size" type="numType" />
            <xs:attribute name="format" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 壁纸元素 -->
    <xs:element name="Wallpaper" type="imageType" />
    <!-- 文本图片 -->
    <xs:element name="ImageSeries">
        <xs:complexType>
            <xs:attributeGroup ref="viewCom" />
            <xs:attribute name="src" type="expression" />
            <xs:attribute name="string" type="expression" />
            <xs:attribute name="mapList" type="stringType" />
            <xs:attribute name="space" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 倒计时 -->
    <xs:element name="CountDownTime">
        <xs:complexType>
            <xs:attribute name="name" type="stringType" />
            <xs:attribute name="date" type="stringType" />
            <xs:attribute name="countDownType" type="stringType" />
            <xs:attribute name="unit" default="day" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 矩形 -->
    <xs:element name="Rectangle">
        <xs:complexType>
            <xs:group maxOccurs="unbounded" minOccurs="0" ref="animationGroup" />
            <xs:attributeGroup ref="geometryGroup" />
            <xs:attribute name="cornerRadius" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 圆形 -->
    <xs:element name="Circle">
        <xs:complexType>
            <xs:group maxOccurs="unbounded" minOccurs="0" ref="animationGroup" />
            <xs:attributeGroup ref="geometryGroup" />
            <xs:attribute name="r" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 椭圆 -->
    <xs:element name="Ellipse">
        <xs:complexType>
            <xs:group maxOccurs="unbounded" minOccurs="0" ref="animationGroup" />
            <xs:attributeGroup ref="geometryGroup" />
        </xs:complexType>
    </xs:element>
    <!-- 直线 -->
    <xs:element name="Line">
        <xs:complexType>
            <xs:group maxOccurs="unbounded" minOccurs="0" ref="animationGroup" />
            <xs:attributeGroup ref="geometryGroup" />
            <xs:attribute name="x1" type="expression" />
            <xs:attribute name="y1" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 扇形 -->
    <xs:element name="Arc">
        <xs:complexType>
            <xs:group maxOccurs="unbounded" minOccurs="0" ref="animationGroup" />
            <xs:attributeGroup ref="geometryGroup" />
            <xs:attribute name="startAngle" type="expression" />
            <xs:attribute name="sweepAngle" type="expression" />
            <xs:attribute name="closure" type="booleanType" />
        </xs:complexType>
    </xs:element>
    <!-- 填充渐变 -->
    <xs:element name="FillGradient" type="shadeType" />
    <!-- 描边渐变 -->
    <xs:element name="StrokeGradient" type="shadeType" />
    <!-- 渐变type -->
    <xs:complexType name="shadeType">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element ref="Linear" />
            <xs:element ref="Radial" />
            <xs:element ref="Sweep" />
        </xs:choice>
    </xs:complexType>
    <!-- 线性渐变的效果：渐变颜色沿渐变轴（x,y,x1,y1）呈线性渐变 -->
    <xs:element name="Linear">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="GradientPoint" />
            </xs:sequence>
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
            <xs:attribute name="x1" type="expression" />
            <xs:attribute name="y1" type="expression" />
            <xs:attribute name="tileMode" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 放射渐变：渐变颜色以设置的渐变中心点为圆心，根据半径r呈环形渐变 -->
    <xs:element name="Radial">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="GradientPoint" />
            </xs:sequence>
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
            <xs:attribute name="r" type="expression" />
            <xs:attribute name="tileMode" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 扫描渐变：渐变颜色以设置的中心点为圆心，呈类似雷达扫描的渐变 -->
    <xs:element name="Sweep">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="GradientPoint" />
            </xs:sequence>
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 渐变子标签:用于设置渐变颜色和渐变位置 -->
    <xs:element name="GradientPoint">
        <xs:complexType>
            <xs:attribute name="color" type="colorType" />
            <xs:attribute name="position" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 组元素组件，可以把图片、按钮、文字、时间等元素包含在组中，同时控制显示隐藏，设置动画等 -->
    <xs:element name="Group">
        <xs:complexType>
            <xs:group maxOccurs="unbounded" minOccurs="0" ref="viewGroup" />
            <xs:attribute name="name" type="stringType" />
            <xs:attribute name="x" default="0" type="expression" />
            <xs:attribute name="y" default="0" type="expression" />
            <xs:attribute name="w" default="0" type="expression" />
            <xs:attribute name="h" default="0" type="expression" />
            <xs:attribute name="width" default="0" type="expression" />
            <xs:attribute name="height" default="0" type="expression" />
            <xs:attribute name="rotation" default="0" type="expression" />
            <xs:attribute name="rotationX" default="0" type="expression" />
            <xs:attribute name="rotationY" default="0" type="expression" />
            <xs:attribute name="angle" default="0" type="expression" />
            <xs:attribute name="angleX" default="0" type="expression" />
            <xs:attribute name="angleY" default="0" type="expression" />
            <xs:attribute name="alpha" default="255" type="expression" />
            <xs:attribute name="active" default="1" type="expression" />
            <xs:attribute name="visibility" default="1" type="expression" />
            <xs:attribute name="layered" type="expression" />
            <xs:attribute name="clip" type="booleanType" />
            <xs:attribute name="align" type="stringType" />
            <xs:attribute name="alignV" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 虚拟屏幕，用于做翻书，百叶窗之类的效果 -->
    <xs:element name="VirtualScreen">
        <xs:complexType>
            <xs:group maxOccurs="unbounded" minOccurs="0" ref="virtualGroup" />
            <xs:attribute name="name" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- The type Drop view -->
    <xs:element name="DropView">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Item" />
                <xs:group ref="virtualGroup" />
            </xs:choice>
            <xs:attribute name="meteor" type="booleanType" />
            <xs:attribute name="isFluttering" type="booleanType" />
            <xs:attribute name="isSpeedRandom" type="booleanType" />
            <xs:attribute name="isRotating" type="booleanType" />
            <xs:attribute name="isRandomSize" type="booleanType" />
            <xs:attribute name="flutterAmplitude" default="1" type="numType" />
            <xs:attribute name="flutterPeriod" default="1" type="numType" />
            <xs:attribute name="lowestSpeed" default="0" type="numType" />
            <xs:attribute name="w" default="0" type="numType" />
            <xs:attribute name="h" default="0" type="numType" />
        </xs:complexType>
    </xs:element>
    <!-- Item -->
    <xs:element name="Item">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Velocity" />
                <xs:element ref="Angle" />
                <xs:element name="Position">
                    <xs:complexType>
                        <xs:attribute name="x" type="numType" />
                        <xs:attribute name="y" type="numType" />
                        <xs:attribute name="isRandom" type="booleanType" />
                    </xs:complexType>
                </xs:element>
                <xs:element name="AngleVelocity">
                    <xs:complexType>
                        <xs:attribute name="angleVelocity" type="xs:float" />
                        <xs:attribute name="lowestAngleVelocity" type="xs:float" />
                        <xs:attribute name="isRandom" type="booleanType" />
                    </xs:complexType>
                </xs:element>
                <xs:element name="Weight">
                    <xs:complexType>
                        <xs:attribute name="value" type="xs:float" />
                        <xs:attribute name="lowestWeight" type="xs:float" />
                        <xs:attribute name="isRandom" type="booleanType" />
                    </xs:complexType>
                </xs:element>
                <xs:element ref="Trigger" />
            </xs:choice>
            <xs:attribute name="number" type="numType" />
            <xs:attribute name="width" type="expression" />
            <xs:attribute name="initSpeedX" type="expression" />
            <xs:attribute name="initSpeedY" type="expression" />
            <xs:attribute name="height" type="expression" />
            <xs:attribute name="src" type="stringType" />
            <xs:attribute name="value" type="expression" />
        </xs:complexType>
    </xs:element>
    <xs:element name="Velocity" substitutionGroup="ItemVelocity" />
    <xs:element name="Angle">
        <xs:complexType>
            <xs:attribute name="isRandom" type="booleanType" />
            <xs:attribute name="angle" type="numType" />
        </xs:complexType>
    </xs:element>
    <!-- 按钮 -->
    <xs:element name="Button" type="buttonType" />
    <xs:element name="SupportPictorialButton" type="buttonType" />
    <!-- Normal状态 -->
    <xs:element name="Normal" type="stateType" />
    <!-- Pressed状态 -->
    <xs:element name="Pressed" type="stateType" />
    <!-- NormalState状态 -->
    <xs:element name="NormalState" type="stateType" />
    <!-- PressedState状态 -->
    <xs:element name="PressedState" type="stateType" />
    <!-- ReachedState状态 -->
    <xs:element name="ReachedState" type="stateType" />
    <!-- 滑动组件 -->
    <xs:element name="Slider">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="StartPoint" />
                <xs:element ref="EndPoint" />
            </xs:choice>
            <xs:attributeGroup ref="sliderAttrs" />
        </xs:complexType>
    </xs:element>
    <!-- StartPoint -->
    <xs:element name="StartPoint" type="pointType" />
    <!-- EndPoint -->
    <xs:element name="EndPoint" type="pointType" />
    <!-- 滑动组件中的标签，用于限制滑块的滑动距离 -->
    <xs:element name="Path">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="Position" />
            </xs:sequence>
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 滑动的点 -->
    <xs:element name="Position">
        <xs:complexType>
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- Intent -->
    <xs:element name="Intent">
        <xs:complexType>
            <xs:attribute name="name" type="stringType" />
            <xs:attribute name="selectIntent" type="stringType" />
            <xs:attribute name="action" type="stringType" />
            <xs:attribute name="uri" type="stringType" />
            <xs:attribute name="type" type="stringType" />
            <xs:attribute name="category" type="stringType" />
            <xs:attribute name="package" type="stringType" />
            <xs:attribute name="class" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 解锁 -->
    <xs:element name="Unlocker">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="StartPoint" />
                <xs:element ref="EndPoint" />
            </xs:choice>
            <xs:attributeGroup ref="sliderAttrs" />
        </xs:complexType>
    </xs:element>
    <!-- 变量：用来保存某些数值，可用于控制其他元素的显示隐藏，也可以用于控制变量动画 -->
    <xs:element name="Var">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="VariableAnimation" />
                <xs:element ref="Trigger" />
                <xs:element ref="Triggers" />
            </xs:choice>
            <xs:attribute name="name" type="stringType" />
            <xs:attribute name="index" type="stringType" />
            <xs:attribute name="type" type="stringType" />
            <xs:attribute name="threshold" type="numType" />
            <xs:attribute name="size" type="stringType" />
            <xs:attribute name="expression" type="stringType" />
            <xs:attribute name="const" type="booleanType" />
            <xs:attribute name="persist" type="booleanType" />
            <xs:attribute name="values" type="stringType" />
            <xs:attribute name="globalPersist" type="stringType" />
            <!-- 音量/亮度进度条 -->
            <xs:attribute name="direction" type="numType" />
            <xs:attribute name="mode" type="numType" />
            <xs:attribute name="topColor" type="colorType" />
            <xs:attribute name="bottomColor" type="colorType" />
            <xs:attribute name="bgSrc" type="stringType" />
            <xs:attribute name="iconSrc" type="stringType" />
            <xs:attribute name="x" default="0" type="expression" />
            <xs:attribute name="y" default="0" type="expression" />
            <xs:attribute name="w" default="0" type="expression" />
            <xs:attribute name="h" default="0" type="expression" />
            <xs:attribute name="progressWidth" type="expression" />
            <xs:attribute name="iconWidth" type="expression" />
            <xs:attribute name="iconHeight" type="expression" />
            <xs:attribute name="roundedCorners" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 变量数组元素 -->
    <xs:element name="VarArray">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Vars" />
                <xs:element ref="Items" />
            </xs:choice>
            <xs:attribute name="type" default="number" />
        </xs:complexType>
    </xs:element>
    <!-- Vars -->
    <xs:element name="Vars">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="Var" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- Items -->
    <xs:element name="Items">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="Item" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- 元素数组 -->
    <xs:element name="Array">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Image" />
                <xs:element ref="Text" />
            </xs:choice>
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
            <xs:attribute name="count" type="expression" />
            <xs:attribute name="indexName" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- VortexView -->
    <xs:element name="VortexView">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:group ref="animationGroup" />
                <xs:element ref="Trigger" />
                <xs:element ref="Triggers" />
            </xs:choice>
            <xs:attributeGroup ref="viewCom" />
            <xs:attribute name="blur" type="numType" />
            <xs:attribute name="src" type="stringType" />
            <xs:attribute name="xfermode" type="stringType" />
            <xs:attribute name="srcExp" type="expression" />
            <xs:attribute name="controlMode" type="expression" />
            <xs:attribute name="srcid" type="numType" />
            <xs:attribute name="useVirtualScreen" type="booleanType" />
            <xs:attribute name="meshx" default="100" type="numType" />
            <xs:attribute name="meshy" default="100" type="numType" />
            <xs:attribute name="speedOfDistance" default="100" type="numType" />
            <xs:attribute name="delayTime" default="100" type="numType" />
            <xs:attribute name="speedOfAngel" default="100" type="numType" />
        </xs:complexType>
    </xs:element>
    <xs:element name="RotateView" substitutionGroup="VortexView" />
    <!-- 蒙板控件，需要与图片元素一起使用 -->
    <xs:element name="Mask">
        <xs:complexType>
            <xs:attributeGroup ref="viewCom" />
            <xs:attribute name="src" type="stringType" />
            <xs:attribute name="hybridMode" type="stringType" />
            <xs:attribute name="absolute" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 笔刷功能类，与Image组合使用,可实现刮刮乐、搽玻璃等效果 -->
    <xs:element name="Paint">
        <xs:complexType>
            <xs:attribute name="weight" type="expression" />
            <xs:attribute name="clear_percent" type="expression" />
            <xs:attribute name="color" type="colorType" />
        </xs:complexType>
    </xs:element>
    <!-- 位移动画 -->
    <xs:element name="PositionAnimation">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element name="Position">
                    <xs:complexType>
                        <xs:attribute name="x" type="numType" />
                        <xs:attribute name="y" type="numType" />
                        <xs:attribute name="time" type="xs:long" />
                        <xs:attribute name="easeType" type="easeAttr" />
                        <xs:attribute name="varSpeedFlag" type="easeAttr" />
                    </xs:complexType>
                </xs:element>
            </xs:choice>
            <xs:attribute name="repeat" type="numType" />
            <xs:attribute name="delay" type="numType" />
        </xs:complexType>
    </xs:element>
    <!-- 透明度动画 -->
    <xs:element name="AlphaAnimation">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Alpha" maxOccurs="unbounded" minOccurs="0">
                    <xs:complexType>
                        <xs:attribute name="a" type="numType" />
                        <xs:attribute name="time" type="xs:long" />
                        <xs:attribute name="easeType" type="easeAttr" />
                        <xs:attribute name="varSpeedFlag" type="easeAttr" />
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="delay" type="numType" />
            <xs:attribute name="repeat" type="numType" />
        </xs:complexType>
    </xs:element>
    <!-- 缩放动画 -->
    <xs:element name="SizeAnimation">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Size" maxOccurs="unbounded" minOccurs="0">
                    <xs:complexType>
                        <xs:attribute name="w" type="numType" />
                        <xs:attribute name="h" type="numType" />
                        <xs:attribute name="time" type="xs:long" />
                        <xs:attribute name="easeType" type="easeAttr" />
                        <xs:attribute name="varSpeedFlag" type="easeAttr" />
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="delay" type="numType" />
            <xs:attribute name="repeat" type="numType" />
        </xs:complexType>
    </xs:element>
    <!-- 旋转动画 -->
    <xs:element name="RotationAnimation">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element name="Rotation" type="rotationType" />
                <xs:element name="RotationX" type="rotationType" />
                <xs:element name="RotationY" type="rotationType" />
            </xs:choice>
            <xs:attribute name="delay" type="numType" />
            <xs:attribute name="repeat" type="numType" />
            <xs:attribute name="angle" type="numType" />
            <xs:attribute name="time" type="xs:long" />
            <xs:attribute name="easeType" type="easeAttr" />
            <xs:attribute name="varSpeedFlag" type="easeAttr" />
        </xs:complexType>
    </xs:element>
    <!-- 帧动画 -->
    <xs:element name="SourcesAnimation">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Source" maxOccurs="unbounded" minOccurs="0">
                    <xs:complexType>
                        <xs:attribute name="x" type="numType" />
                        <xs:attribute name="y" type="numType" />
                        <xs:attribute name="src" type="stringType" use="required" />
                        <xs:attribute name="time" type="xs:long" />
                        <xs:attribute name="easeType" type="easeAttr" />
                        <xs:attribute name="varSpeedFlag" type="easeAttr" />
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="delay" type="numType" />
            <xs:attribute name="repeat" type="numType" />
        </xs:complexType>
    </xs:element>
    <!-- 变量动画:基础命令，让一个变量在规定时间内变化动画 -->
    <xs:element name="VariableAnimation">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="AniFrame" />
            </xs:sequence>
            <xs:attribute name="delay" type="numType" />
            <xs:attribute name="repeat" type="numType" />
            <xs:attribute name="easeType" type="easeAttr" />
            <xs:attribute name="varSpeedFlag" type="easeAttr" />
        </xs:complexType>
    </xs:element>
    <!-- AniFrame -->
    <xs:element name="AniFrame">
        <xs:complexType>
            <xs:attribute name="time" type="expression" />
            <xs:attribute name="value" type="expression" />
            <xs:attribute name="easeType" type="easeAttr" />
            <xs:attribute name="varSpeedFlag" type="easeAttr" />
        </xs:complexType>
    </xs:element>
    <!-- 传感器 -->
    <xs:element name="VariableBinders">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="ContentProviderBinder" />
                <xs:element ref="SensorBinder" />
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <!-- 数据查询组件 -->
    <xs:element name="ContentProviderBinder">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="Variable" />
            </xs:sequence>
            <xs:attribute name="uri" type="stringType" />
            <xs:attribute name="uriFormat" type="stringType" />
            <xs:attribute name="uriParas" type="stringType" />
            <xs:attribute name="columns" type="stringType" />
            <xs:attribute name="where" type="stringType" />
            <xs:attribute name="args" type="stringType" />
            <xs:attribute name="order" type="stringType" />
            <xs:attribute name="countName" type="stringType" />
            <xs:anyAttribute processContents="lax" />
        </xs:complexType>
    </xs:element>
    <!-- 传感器数据绑定组件 -->
    <xs:element name="SensorBinder">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="Variable" />
            </xs:sequence>
            <xs:attribute name="type" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!--Variable -->
    <xs:element name="Variable">
        <xs:complexType>
            <xs:attribute name="name" type="stringType" />
            <xs:attribute name="column" type="stringType" />
            <xs:anyAttribute processContents="lax" />
        </xs:complexType>
    </xs:element>
    <!-- 高级动态帧率 -->
    <xs:element name="VariableFramerate">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="VariablePoint" />
            </xs:sequence>
            <xs:attribute name="visibility" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- The type Control point -->
    <xs:element name="VariablePoint">
        <xs:complexType>
            <xs:attribute name="time" type="stringType" />
            <xs:attribute name="frameRate" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 跟手拖尾粒子 -->
    <xs:element name="ParticleView">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="PathData" />
            </xs:sequence>
            <xs:attribute name="w" type="expression" />
            <xs:attribute name="h" type="expression" />
            <xs:attribute name="count" type="numType" />
            <xs:attribute name="move_radius" type="expression" />
            <xs:attribute name="speek_sec" type="expression" />
            <xs:attribute name="reduce_size" type="numType" />
            <xs:attribute name="set_path" type="expression" />
            <xs:attribute name="src" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 路径 -->
    <xs:element name="PathData">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="Range" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- Range包含一个小方形的坐标数据 -->
    <xs:element name="Range">
        <xs:complexType>
            <xs:attribute name="rect" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 水波纹 -->
    <xs:element name="WaterWallpaper">
        <xs:complexType>
            <xs:group maxOccurs="unbounded" minOccurs="0" ref="animationGroup" />
            <xs:attributeGroup ref="viewCom" />
            <xs:attribute name="blur" type="numType" />
            <xs:attribute name="src" type="stringType" />
            <xs:attribute name="xfermode" type="stringType" />
            <xs:attribute name="srcExp" type="expression" />
            <xs:attribute name="srcid" type="numType" />
            <xs:attribute name="useVirtualScreen" type="booleanType" />
            <xs:attribute name="color" type="expression" />
            <xs:attribute name="xmesh" type="expression" />
            <xs:attribute name="ymesh" type="expression" />
            <xs:attribute name="torsion" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 网格化功能 -->
    <xs:element name="MeshImage">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Translation" />
                <xs:element ref="Alpha" />
                <xs:element ref="Scale" />
                <xs:element ref="Rotate" />
                <xs:element ref="SinMotion" />
            </xs:choice>
            <xs:attributeGroup ref="viewCom" />
            <xs:attribute name="mesh" type="stringType" />
            <xs:attribute name="src" type="stringType" />
            <xs:attribute name="center_point" type="expression" />
            <xs:attribute name="sensor" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 位移 -->
    <xs:element name="Translation">
        <xs:complexType>
            <xs:attribute name="duration" type="expression" />
            <xs:attribute name="repeat" type="expression" />
            <xs:attribute name="delay" type="expression" />
            <xs:attribute name="factor" type="expression" />
            <xs:attribute name="values" type="stringType" />
            <xs:attribute name="max_distance_x" type="expression" />
            <xs:attribute name="max_distance_y" type="expression" />
            <xs:attribute name="no_move_distance_x" type="expression" />
            <xs:attribute name="no_move_distance_y" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 透明度 -->
    <xs:element name="Alpha">
        <xs:complexType>
            <xs:attribute name="duration" type="expression" />
            <xs:attribute name="repeat" type="expression" />
            <xs:attribute name="delay" type="expression" />
            <xs:attribute name="values" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 缩放 -->
    <xs:element name="Scale">
        <xs:complexType>
            <xs:attribute name="duration" type="expression" />
            <xs:attribute name="repeat" type="expression" />
            <xs:attribute name="delay" type="expression" />
            <xs:attribute name="values" type="stringType" />
            <xs:attribute name="no_move_distance_x" type="expression" />
            <xs:attribute name="no_move_distance_y" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 旋转 -->
    <xs:element name="Rotate">
        <xs:complexType>
            <xs:attribute name="duration" type="expression" />
            <xs:attribute name="repeat" type="expression" />
            <xs:attribute name="delay" type="expression" />
            <xs:attribute name="values" type="stringType" />
            <xs:attribute name="no_move_distance_x" type="expression" />
            <xs:attribute name="no_move_distance_y" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 水波荡漾 -->
    <xs:element name="SinMotion">
        <xs:complexType>
            <xs:attribute name="a" type="expression" />
            <xs:attribute name="factor_k" type="expression" />
            <xs:attribute name="no_move_distance_x" type="expression" />
            <xs:attribute name="no_move_distance_y" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 跑马灯 -->
    <xs:element name="Marquee">
        <xs:complexType>
            <xs:attributeGroup ref="viewCom" />
            <xs:attribute name="state" type="expression" />
            <xs:attribute name="showTime" type="expression" />
            <xs:attribute name="deltaAngle" type="expression" />
            <xs:attribute name="color" type="colorType" />
            <xs:attribute name="marqueeColor" type="colorType" />
            <xs:attribute name="marqueePos" type="expression" />
            <xs:attribute name="weight" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 翻书页 -->
    <xs:element name="FlipBook">
        <xs:complexType>
            <xs:group maxOccurs="unbounded" minOccurs="0" ref="animationGroup" />
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
            <xs:attribute name="color" type="colorType" />
            <xs:attribute name="src" type="stringType" />
            <xs:attribute name="bgsrc" type="stringType" />
            <xs:attribute name="useVirtualScreen" type="booleanType" />
            <xs:attribute name="srcExp" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 情景动效 -->
    <xs:element name="Situations">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="situation" />
