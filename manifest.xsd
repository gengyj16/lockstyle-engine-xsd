        </xs:restriction>
    </xs:simpleType>
    <!-- 表达式 四则运算不能连续出现 todo:括号成对 -->
    <xs:simpleType name="expression">
        <xs:restriction base="stringType">
            <xs:pattern value="[^+\-*/]*([+\-*/][^+\-*/]+)*[+\-*/]?[^+\-*/]*"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- 字符串 -->
    <xs:simpleType name="stringType">
        <xs:restriction base="xs:string">
            <xs:pattern value="([^>])+" />
        </xs:restriction>
    </xs:simpleType>
    <!-- 颜色限定 未定 -->
    <xs:simpleType name="colorType">
        <xs:restriction base="stringType" />
    </xs:simpleType>
    <!-- 数字 -->
    <xs:simpleType name="numType">
        <xs:restriction base="stringType"></xs:restriction>
    </xs:simpleType>
    <!-- boolean:true或false -->
    <xs:simpleType name="booleanType">
        <xs:restriction base="stringType">
            <xs:enumeration value="true" />
            <xs:enumeration value="false" />
        </xs:restriction>
    </xs:simpleType>
    <!-- 音乐控制器 -->
    <xs:element name="MediaControl">
        <xs:complexType>
            <xs:attribute name="packageName" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 歌词 -->
    <xs:element name="LrcView">
        <xs:complexType>
            <xs:attributeGroup ref="viewCom" />
            <xs:attribute name="color" type="colorType" />
            <xs:attribute name="highColor" type="colorType" />
            <xs:attribute name="size" type="stringType" />
            <xs:attribute name="highSize" type="stringType" />
            <xs:attribute name="dividerHeight" type="stringType" />
            <xs:attribute name="lineNum" type="stringType" />
            <xs:attribute name="maxLines" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 专辑封面 -->
    <xs:element name="MediaIcon" type="imageType" />
    <!-- 音乐进度条 -->
    <xs:element name="MediaProgressbar">
        <xs:complexType>
            <xs:attributeGroup ref="viewCom" />
            <xs:attribute name="topColor" type="colorType" />
            <xs:attribute name="bottomColor" type="colorType" />
            <xs:attribute name="bgSrc" type="stringType" />
            <xs:attribute name="iconSrc" type="stringType" />
            <xs:attribute name="progressWidth" type="expression" />
            <xs:attribute name="iconWidth" type="expression" />
            <xs:attribute name="iconHeight" type="expression" />
            <xs:attribute name="isRightAngle" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 音乐控制命令 -->
    <xs:element name="MediaCommand">
        <xs:complexType>
            <xs:attribute name="command" type="mediaCommandType" />
            <xs:attribute name="voiceStep100" type="expression" />
            <xs:attribute name="adid" type="stringType" />
            <xs:attributeGroup ref="commandAttrs" />
        </xs:complexType>
    </xs:element>

    <!-- 音乐控制命令类型 -->
    <xs:simpleType name="mediaCommandType">
        <xs:restriction base="stringType">
            <xs:enumeration value="mediaPlay" />
            <xs:enumeration value="mediaPause" />
            <xs:enumeration value="mediaLike" />
            <xs:enumeration value="mediaDislike" />
            <xs:enumeration value="skipToPrevious" />
            <xs:enumeration value="skipToNext" />
            <xs:enumeration value="addVoice" />
            <xs:enumeration value="subVoice" />
            <xs:enumeration value="mediaRepeatMode" />
        </xs:restriction>
    </xs:simpleType>

    <xs:element name="Pets">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="PetImage" />
                <xs:element ref="PetTask" />
            </xs:choice>
        </xs:complexType>
    </xs:element>

    <xs:element name="PetImage">
        <xs:complexType>
            <xs:attribute name="name" type="stringType" />
            <xs:attribute name="from" type="numType" />
            <xs:attribute name="to" type="numType" />
            <xs:attribute name="time" type="numType" />
            <xs:attribute name="repeat" type="numType" />
            <xs:attribute name="grade" type="numType" />
            <xs:attribute name="dir" type="stringType" />
            <xs:attribute name="defaultDir" type="stringType" />
            <xs:attribute name="record" type="numType" />
            <xs:attribute name="requestCode" type="numType" />
            <xs:attribute name="skillName" type="stringType" />
            <xs:attribute name="interrupt" type="numType" />
        </xs:complexType>
    </xs:element>

    <xs:element name="PetTask">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element name="Item">
                    <xs:complexType>
                        <xs:choice maxOccurs="unbounded" minOccurs="0">
                            <xs:element ref="PetImage" />
                        </xs:choice>
                        <xs:attribute name="mapList" type="stringType" />
                    </xs:complexType>
                </xs:element>
            </xs:choice>
            <xs:attribute name="priority" type="xs:float" />
            <xs:attribute name="mapList" type="stringType" />
            <xs:attribute name="condition" type="expression" />
            <xs:attribute name="model" type="stringType" />
            <xs:attribute name="deleteAfterUser" type="booleanType" />
            <xs:attribute name="loop" type="booleanType" />
            <xs:attribute name="rand" type="booleanType" />
        </xs:complexType>
    </xs:element>

    <xs:element name="PetGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="PetText" />
            </xs:choice>
            <xs:attribute name="duration" type="numType" />
            <xs:attribute name="bg" type="stringType" />
            <xs:attribute name="offx" type="expression" />
            <xs:attribute name="offy" type="expression" />
        </xs:complexType>
    </xs:element>

    <xs:element name="PetText">
        <xs:complexType>
            <xs:attribute name="requestCode" type="numType" />
            <xs:attribute name="format" type="expression" />
            <xs:attribute name="text" type="expression" />
            <xs:attribute name="color" type="colorType" />
            <xs:attribute name="size" type="numType" />
            <xs:attribute name="marginLeft" type="numType" />
            <xs:attribute name="marginRight" type="numType" />
            <xs:attribute name="marginTop" type="numType" />
            <xs:attribute name="marginBottom" type="numType" />
            <xs:attribute name="bold" type="booleanType" />
            <xs:attribute name="textalign" type="stringType" />
            <xs:attribute name="repeatMsg" type="stringType" />
            <xs:attribute name="clickable" type="stringType" />
        </xs:complexType>
    </xs:element>

    <xs:element name="PetSkill">
        <xs:complexType>
            <xs:attribute name="icon" type="stringType" />
            <xs:attribute name="skill" type="stringType" />
            <xs:attribute name="rank" type="numType" />
            <xs:attribute name="common" type="booleanType" />
            <xs:attribute name="name" type="stringType" />
        </xs:complexType>
    </xs:element>

    <xs:element name="EditCommand">
        <xs:complexType>
            <xs:attribute name="editId" type="stringType" />
            <xs:attributeGroup ref="commandAttrs" />
        </xs:complexType>
    </xs:element>
</xs:schema>
