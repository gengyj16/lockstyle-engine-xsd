            <xs:attribute name="duration" default="1000" type="expression" />
            <xs:attribute name="style" default="0" type="expression" />
            <xs:attribute name="shaketime" default="150" type="expression" />
            <xs:attribute name="radius" default="2" type="expression" />
            <xs:attribute name="scale" type="numType" />
