    <!--  常用形状渲染功能 -->
    <xs:element name="Graph">
        <xs:complexType>
            <xs:attribute name="x" type="expression" />
            <xs:attribute name="y" type="expression" />
            <xs:attribute name="radius" type="expression" />
            <xs:attribute name="left" type="expression" />
            <xs:attribute name="top" type="expression" />
            <xs:attribute name="right" type="expression" />
            <xs:attribute name="bottom" type="expression" />
            <xs:attribute name="pathname" type="stringType" />
            <xs:attribute name="shape" type="stringType" />
            <xs:attribute name="wave" type="stringType" />
