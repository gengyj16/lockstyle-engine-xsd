            <xs:attribute name="vars" type="stringType" />
            <xs:attribute name="affect" type="stringType" />
            <xs:attribute name="delay" type="stringType" />
            <xs:attribute name="restrict" type="stringType" />
            <xs:attribute name="sync" type="expression" />
