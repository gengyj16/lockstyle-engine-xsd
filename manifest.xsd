            <xs:attribute name="coversrc" type="stringType" />
            <xs:attribute name="slidingAppear" type="booleanType" />
            <xs:attribute name="srcid" type="expression" />
            <xs:attribute name="useVirtualScreen" default="false" type="booleanType" />
            <xs:attribute name="srcExp" type="expression" />
        </xs:complexType>
    </xs:element>
