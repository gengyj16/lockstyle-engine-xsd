    <!--  全景动效 -->
    <xs:element name="VR">
        <xs:complexType>
            <xs:attribute name="src" type="stringType" />
            <xs:attribute name="srcid" default="0" type="expression" />
            <xs:attribute name="imageType" type="stringType" />
            <xs:attribute name="touchType" default="0" type="expression" />
        </xs:complexType>
