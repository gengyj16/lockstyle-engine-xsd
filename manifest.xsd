    <xs:element name="situation">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Image" />
                <xs:element ref="VirtualScreen" />
            </xs:choice>
            <xs:attribute name="name" type="stringType" />
            <xs:attribute name="start" type="stringType" />
            <xs:attribute name="end" type="stringType" />
            <xs:attribute name="visibility_situation" type="expression" />
