            <xs:attribute name="style" default="1" type="expression" />
            <xs:attribute name="shaketime" default="150" type="expression" />
            <xs:attribute name="path" type="stringType" />
            <xs:attribute name="reverse" type="booleanType" />
            <xs:attribute name="unlock" type="booleanType" />
            <xs:attribute name="src" type="stringType" />
            <xs:attribute name="useVirtualScreen" default="false" type="booleanType" />
            <xs:attribute name="shape" default="0" type="expression" />
            <xs:attribute name="radius" default="2" type="expression" />
            <xs:attribute name="start" type="expression" />
            <xs:attribute name="rect" type="expression" />
            <xs:attribute name="controlMode" type="expression" />
        </xs:complexType>
    </xs:element>
    <!-- 路径解析 -->
    <xs:element name="PathUtil">
        <xs:complexType>
            <xs:attribute name="name" type="stringType" />
            <xs:attribute name="path" type="stringType" />
        </xs:complexType>
    </xs:element>
    <!-- 路径动画 -->
    <xs:element name="PathRun">
        <xs:complexType>
            <xs:attributeGroup ref="viewCom" />
            <xs:attribute name="color" type="colorType" />
            <xs:attribute name="highlightlen" default="0" type="expression" />
            <xs:attribute name="weight" default="5" type="expression" />
            <xs:attribute name="speed" default="20" type="expression" />
            <xs:attribute name="loop" type="booleanType" />
            <xs:attribute name="fillflag" type="booleanType" />
            <xs:attribute name="loopcount" default="0" type="expression" />
            <xs:attribute name="async" type="booleanType" />
            <xs:attribute name="src" type="stringType" />
            <xs:attribute name="imageWidth" default="40" type="expression" />
            <xs:attribute name="imageHeight" default="40" type="expression" />
            <xs:attribute name="shake" type="booleanType" />
            <xs:attribute name="imageTransX" default="0" type="expression" />
            <xs:attribute name="imageTransY" default="0" type="expression" />
            <xs:attribute name="imageTransformStyle" default="0" type="expression" />
